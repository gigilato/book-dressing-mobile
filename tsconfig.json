{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "allowJs": false,
    "baseUrl": "./src",
    "experimentalDecorators": true,
    "module": "esnext",
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "sourceMap": true,
    "target": "esnext",
    "paths": {
      "@components": [
        "components/index"
      ],
      "@components/*": [
        "components/*"
      ],
      "@constants/*": [
        "constants/*"
      ],
      "@config": [
        "config/index"
      ],
      "@services": [
        "services/index"
      ],
      "@services/*": [
        "services/*"
      ],
      "@screens": [
        "screens/index"
      ],
      "@screens/*": [
        "screens/*"
      ],
      "@hooks": [
        "hooks/index"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@navigation": [
        "navigation/index"
      ],
      "@navigation/*": [
        "navigation/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@assets": [
        "assets/index"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@theme": [
        "theme/index"
      ],
      "@api": [
        "api/index"
      ],
      "@api/*": [
        "api/*"
      ]
    },
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": [
      "node"
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "index.js",
    "src"
  ],
  "extends": "expo/tsconfig.base"
}
